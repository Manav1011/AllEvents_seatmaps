eventsHandler = {
    haltEventListeners: [
        'touchstart',
        'touchend',
        'touchmove',
        'touchleave',
        'touchcancel'
    ],
    init: function(options) {
        var instance = options.instance,
            initialScale = 1,
            pannedX = 0,
            pannedY = 0 // Init Hammer // Listen only for pointer and touch events 
        this.hammer = Hammer(options.svgElement, {
                inputClass: Hammer.SUPPORT_POINTER_EVENTS ? Hammer.PointerEventInput : Hammer.TouchInput
            })
            // Enable pinch 
        this.hammer.get('pinch').set({
                enable: true
            })
            // Handle double tap 
        this.hammer.on('doubletap',
                function(ev) {
                    //instance.zoomIn()
                })
            // Handle pan 
        this.hammer.on('panstart panmove', function(ev) {
                // On pan start reset panned variables 
                if (ev.type === 'panstart') {
                    pannedX = 0;
                    pannedY = 0;
                } // Pan only the difference 
                instance.panBy({ x: ev.deltaX - pannedX, y: ev.deltaY - pannedY });
                pannedX = ev.deltaX;
                pannedY = ev.deltaY;
            }) // Handle pinch 
        this.hammer.on('pinchstart pinchmove', function(ev) {
                // On pinch start remember initial zoom 
                if (ev.type === 'pinchstart') {
                    initialScale = instance.getZoom();
                    instance.zoomAtPoint(initialScale * ev.scale, ev.center);
                }
                instance.zoomAtPoint(initialScale * ev.scale, ev.center);
            })
            // Prevent moving the page on some devices when panning over SVG 
        options.svgElement.addEventListener('touchmove', function(e) { e.preventDefault(); });
    },
    destroy: function() {
        this.hammer.destroy()
    }
}

aeseatMap.api_response = {};
aeseatMap.event = {};

aeseatMap.map_json_to_svg = function(callback) {
    //map data over seatmap
    var seatmapdata = aeseatMap.api_response;
    //map data over seatmap
    processSeat(seatmapdata.seatmap, 0, seatmapdata.seatmap[0], callback)
        // $(seatmapdata.seatmap).each(function(index, el) {
        //  //   console.log(el);

    // });
}
function processSeat(seatmaparr, index, el, callback) {
    //console.log("start"+index);
    for (var i = 0; i < 20; i++) {
        try {
            var t_id = el.id;
            if (el.type == "seat") {
                // console.log(aeseatMap.event.ticket_types_keyval[el.ticket_type_id]);
                // var seat = aeseatMap.getSeatObject(t_id);
                var seat = {};
                seat.t_id = t_id;
                seat.seatDomEl = aeseatMap.seat_map_display_elm.find("#" + seat.t_id);
                seat.svgEl = SVG.adopt(seat.seatDomEl[0]);
                seat.seatData = seat.seatDomEl.data();
                seat.seatSvgEl = seat.svgEl;
                seat.seatSvgElBack = SVG.adopt(seat.seatDomEl.find(".seat")[0]);
                seat.seatSvgElText = SVG.adopt(seat.seatDomEl.find("text")[0]);
                seat.set_seatColor = function(seatColor) {
                    aeseatMap.set_seatColor(seat, seatColor);
                }
                seat.set_text = function(newtext) {
                    aeseatMap.set_text(seat.svgEl, newtext);
                }
                if (seat == false) {
                    return; }
                //binding data
                //seatSvgEl.on('click', seat_click);
                seat.seatDomEl.addClass("seat " + el.status);
                seat.seatDomEl.data({
                                        'data' : el,
                                        'ticket_type_id' : el.ticket_type_id,
                                        'name' : el.name,
                                        'status' : el.status
                                    });
                //set colors based on status
                seat.seatSvgElBack.fill(aeseatMap.colorMap(el.status));

                //tooltips
                var text = "";
                var title = "";
                var theme = "default";
                if (aeseatMap.event.ticket_types_keyval[el.ticket_type_id] == undefined) {
                    text = el.name;
                } else {
                    var t_type = aeseatMap.event.ticket_types_keyval[el.ticket_type_id];
                    title += t_type.type;
                    title += " - " + el.name;
                    text += t_type.currency + " " + addCommas(parseFloat(t_type.price));
                }
                if (el.status !== "available") {
                    if (el.status === "hold") {
                        title = "Another customer is currently buying this ticket.";
                        theme = "orange";
                        text = "";
                    } else {
                        title = "Not available";
                        theme = "red";
                        text = "";
                        seat.seatDomEl.removeClass("available");
                    }

                } else {
                    theme = "bootstrap";
                }

                setToolTip(seat.seatDomEl, text, title, theme);

                if (el.status == "available" && typeof(el.data.seatColor) !== undefined) {
                    seat.seatDomEl.data('seatColor', el.data.seatColor);
                    seat.set_seatColor(el.data.seatColor);
                }else{
                    seat.seatSvgElBack.stroke({ width: 0 });
                }
            }
        } catch (e) {
            console.log(e);
        }
        //console.log("in"+index);
        if((index + 1) > seatmaparr.length){
            break;
        }
        index = index + 1;
        el = seatmaparr[index];
    }
    //console.log("out"+index);
    if (index < seatmaparr.length) {
        setTimeout(function() { processSeat(seatmaparr, index, seatmaparr[index], callback) }, 0);
    } else {
        if (typeof(callback) === "function") {
            callback();
        }
    }
}
aeseatMap.colorMap = function(status) {
    var color = "#aaa";
    switch (status) {
        case 'available':
            color = "#1168C5";
            break;
        case 'sold':
            color = "#d2d2d2";
            break;
        case 'hold':
            color = "#dda";
            break;
        case 'blocked':
            color = "#d2d2d2";
            break;
        case 'selected':
            color = "#ffffff";
            break;
        default:
            color = "#aaa";;
    }
    return color;
}

var addCommas = function(nStr) {
    nStr += '';
    x = nStr.split('.');
    x1 = x[0];
    x2 = x.length > 1 ? '.' + x[1] : '';
    var rgx = /(\d+)(\d{3})/;
    while (rgx.test(x1)) {
        x1 = x1.replace(rgx, '$1' + ',' + '$2');
    }
    return x1 + x2;
};
var params = {
    content: {
        "text": "",
        "title": ""
    },
    position: {
        at: 'top center',
        my: 'bottom center'
    },
    style: {
        classes: ""
    },
    show: {
        event: "click touchend mouseenter"
    },
    hide: {
        event: 'unfocus blur mouseleave'
    }
};
var setToolTip = function(selectorOrEle, text, title, theme) {
    text = text || "";
    title = title || "";
    theme = theme || "";
    var theme_class = "qtip-shadow qtip-" + theme;
    params.content.text = text;
    params.content.title = title;
    params.style.classes = theme_class;
    if (text == "" && title != "") {
        text = title;
        title = "";
        params.content.text = text;
    }
    if (title == "") {
        delete params.content.title;
    }
    if (typeof(selectorOrEle) === "string") {
        $(selectorOrEle).qtip(params);
    } else {
        selectorOrEle.qtip(params);
    }
}
    
var get_event_seatmap = function(event_id, callback) {
    callback = callback || function() {};
    var request_object = {
        "event_id": ""
    };
    request_object.event_id = event_id;
    $.ajax({
            url: 'https://allevents.in/api/index.php/tickets/web/get_seatmap_layout/'+event_id+'?static=true',
            type: 'GET'
        })
        .done(function(response) {
            if (response.error == 0) {
                callback(response);
            } else {
                callback(false);
            }

        })
        .fail(function() {
            console.log("error");
            callback(false);
        })
}

var get_live_seatmap = function(event_id, callback) {
    callback = callback || function() {};
    var request_object = {
        "event_id": ""
    };
    request_object.event_id = event_id;
    $.ajax({
            url: 'https://allevents.in/api/index.php/tickets/web/get_live_seatmap',
            type: 'POST',
            data: JSON.stringify(request_object),
        })
        .done(function(response) {
            console.log(response)
            if (response.error == 0) {
                callback(response);
            } else {
                callback(false);
            }

        })
        .fail(function() {
            console.log("error");
            callback(false);
        })
}
aeseatMap.init = function(callback) {
    get_event_seatmap(current_event_id, function(data) {
        //   console.log(data.data);

        aeseatMap.selected_seats_object = Object();

        //update svg map
        if (data.data.layout_xml !== "") {
            $(seat_map_display).html(data.data.layout_xml);
            aeseatMap.seat_map_display_elm = $($.parseHTML(data.data.layout_xml));
            $("#svg_seat_map")[0].setAttribute("width", $("body").width());
            $("#svg_seat_map")[0].setAttribute("height", $("body").height() - ($("#seatmapview .container-fluid").first().outerHeight() + $(".seatmap-footer").first().outerHeight()));
            try{window.zoomSeatmap.destroy();}catch(e){};
                window.zoomSeatmap = svgPanZoom('#seat_map_display svg', {
                    zoomEnabled: true,
                    controlIconsEnabled: true,
                    dblClickZoomEnabled: false,
                    fit: 1,
                    center: true,
                    maxZoom: 5,
                    customEventsHandler: eventsHandler
                });
                zoomSeatmap.resize();
                zoomSeatmap.fit();
                zoomSeatmap.center();
        }

        get_live_seatmap(current_event_id, function(data) {
            aeseatMap.api_response = data.data;
            aeseatMap.event.ticket_types_keyval = data.data.ticket_types;
            console.time("processing_seats");
            aeseatMap.map_json_to_svg(function() {
                console.timeEnd("processing_seats");
                $("#seat_map_display").html("").append(aeseatMap.seat_map_display_elm);
                $("#svg_seat_map")[0].setAttribute("width", $("body").width());
                $("#svg_seat_map")[0].setAttribute("height", $("body").height() - ($("#seatmapview .container-fluid").first().outerHeight() + $(".seatmap-footer").first().outerHeight()));
                try{window.zoomSeatmap.destroy();}catch(e){};
                window.zoomSeatmap = svgPanZoom('#seat_map_display svg', {
                    zoomEnabled: true,
                    controlIconsEnabled: true,
                    dblClickZoomEnabled: false,
                    fit: 1,
                    center: true,
                    maxZoom: 5,
                    customEventsHandler: eventsHandler
                });
                zoomSeatmap.resize();
                zoomSeatmap.fit();
                zoomSeatmap.center();
                $(window).resize(function() {
                    try{window.zoomSeatmap.destroy();}catch(e){};
                    window.zoomSeatmap = svgPanZoom('#seat_map_display svg', {
                        zoomEnabled: true,
                        controlIconsEnabled: true,
                        dblClickZoomEnabled: false,
                        fit: 1,
                        center: true,
                        maxZoom: 5,
                        customEventsHandler: eventsHandler
                    });
                    $("#svg_seat_map")[0].setAttribute("width", $("#seat_map_display").width());
                    $("#svg_seat_map")[0].setAttribute("height", $("body").height() - ($("#seatmapview .container-fluid").first().outerHeight() + $(".seatmap-footer").first().outerHeight()));
                    zoomSeatmap.resize();
                    zoomSeatmap.fit();
                    zoomSeatmap.center();
                })
                if (typeof(callback) === "function") {
                    callback();
                }
            });
        })

    });
}